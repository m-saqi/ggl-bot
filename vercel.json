{
  "version": 2,
  "builds": [
    {
      "src": "api/automate.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/automate.js": {
      "maxDuration": 10,
      "memory": 1024 
    }
  },
  "routes": [
    {
      "src": "/api/automate",
      "dest": "/api/automate.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
